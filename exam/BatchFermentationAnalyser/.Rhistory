readBatchDatasets <- function(x) {
mylist <- vector(mode = "list", length = length(x))
e = 1
for (i in x) {
mylist[[e]] <- read.table(i,
header = TRUE,
colClasses = "numeric")
e = e + 1
}
return(mylist)
}
